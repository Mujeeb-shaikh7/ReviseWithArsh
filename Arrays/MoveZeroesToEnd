class Solution {
public:
    void moveZeroes(vector<int>& input) {
          int k=0,temp;
        int n=input.size();
    for(int i=0;i<n;i++){
        if(input[i]!=0){
            temp=input[i];
            input[i]=input[k];
            input[k]=temp;
            k++;
            }
        else{
        }
    }
    }
};
